<?xml version="1.0" encoding="utf-8"?>
<layout>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".View.ChangePassFragment">

  <EditText
      android:id="@+id/txtConfirmNewPass"
      android:layout_width="match_parent"
      android:layout_height="60dp"
      android:layout_marginStart="16dp"
      android:layout_marginTop="16dp"
      android:layout_marginEnd="16dp"
      android:background="@drawable/border"
      android:drawableStart="@drawable/ic_password_blue"
      android:drawableEnd="@drawable/ic_eye"
      android:drawablePadding="16dp"
      android:ems="10"
      android:hint="Confirm Password"
      android:inputType="textPassword"
      android:paddingHorizontal="16dp"
      android:textAlignment="textStart"
      android:textColor="@color/black"
      android:textStyle="bold"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView17" />

  <TextView
      android:id="@+id/textView17"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="16dp"
      android:layout_marginTop="24dp"
      android:fontFamily="@font/poppins_bold"
      android:text="Confirm New Password"
      android:textColor="#017FFF"
      android:textSize="16sp"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/txtNewPass" />

  <EditText
      android:id="@+id/txtNewPass"
      android:layout_width="match_parent"
      android:layout_height="60dp"
      android:layout_marginStart="16dp"
      android:layout_marginTop="16dp"
      android:layout_marginEnd="16dp"
      android:background="@drawable/border"
      android:drawableStart="@drawable/ic_password_blue"
      android:drawableEnd="@drawable/ic_eye"
      android:drawablePadding="16dp"
      android:ems="10"
      android:hint="New Password"
      android:inputType="textPassword"
      android:paddingHorizontal="16dp"
      android:textAlignment="textStart"
      android:textColor="@color/black"
      android:textStyle="bold"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView16" />

  <TextView
      android:id="@+id/textView16"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="16dp"
      android:layout_marginTop="24dp"
      android:fontFamily="@font/poppins_bold"
      android:text="New Password"
      android:textColor="#017FFF"
      android:textSize="16sp"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/txtOldPass" />

  <TextView
      android:id="@+id/textView11"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="16dp"
      android:layout_marginTop="16dp"
      android:fontFamily="@font/poppins_bold"
      android:text="Old Password"
      android:textColor="#017FFF"
      android:textSize="16sp"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/hr16" />

  <EditText
      android:id="@+id/txtOldPass"
      android:layout_width="match_parent"
      android:layout_height="60dp"
      android:layout_marginStart="16dp"
      android:layout_marginTop="16dp"
      android:layout_marginEnd="16dp"
      android:background="@drawable/border"
      android:drawableStart="@drawable/ic_password_blue"
      android:drawableEnd="@drawable/ic_eye"
      android:drawablePadding="16dp"
      android:ems="10"
      android:hint="Old Password"

      android:inputType="textPassword"
      android:paddingHorizontal="16dp"
      android:textAlignment="textStart"
      android:textColor="@color/black"
      android:textStyle="bold"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView11" />

  <Button
      android:id="@+id/btnSave"
      android:layout_width="match_parent"
      android:layout_height="60dp"
      android:layout_marginStart="16dp"
      android:layout_marginEnd="16dp"
      android:backgroundTint="#03A9F4"
      android:text="Save"
      android:textColor="@color/white"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent" />

  <TextView
      android:id="@+id/tvNameTop10"
      android:layout_width="220dp"
      android:layout_height="30dp"

      android:layout_marginStart="16dp"
      android:text="Change Password"
      android:textColor="#223263"
      android:textSize="20dp"
      android:textStyle="bold"
      app:layout_constraintBottom_toTopOf="@+id/hr16"
      app:layout_constraintStart_toEndOf="@+id/ibtnBack9"
      app:layout_constraintTop_toTopOf="parent" />

  <ImageButton
      android:id="@+id/ibtnBack9"
      android:layout_width="40dp"
      android:layout_height="40dp"
      android:layout_marginStart="16dp"
      android:layout_marginTop="16dp"
      android:backgroundTint="#FFFFFF"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/left" />

  <View
      android:id="@+id/hr16"
      android:layout_width="match_parent"
      android:layout_height="2dp"
      android:layout_marginTop="69dp"
      android:background="#EBF0FF"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>
</layout>